function _CallRandomImg(){fetch("/api/random-img-sign").then(r=>{if(!r.ok)throw new Error("Error calling API");return r.json()}).then(r=>{$(".sign-img img").attr("data-src",r.img).removeClass("hide-sm")}).catch(r=>{notyf("Error connection to callback random-img-sign!","danger")})}function _CallQRCode(){fetch("/api/qr-code?data="+encodeURIComponent(window.location.href)+"&size="+$(".qrcode").data("size")).then(r=>{if(!r.ok)throw new Error("Error calling API");return r.arrayBuffer()}).then(r=>{$(".qrcode").html('<img src="data:image/png;base64,'+arrayBufferBase64(r)+'" alt="QR Code">')}).catch(r=>{notyf("Error connection to callback qr-code!","danger")})}function arrayBufferBase64(r){return btoa(Array.from(new Uint8Array(r)).map(r=>String.fromCharCode(r)).join(""))}_CallRandomImg(),_CallQRCode(),window.location.href.indexOf("view-code")>-1&&($(".current-menu-item").removeClass("current-menu-item"),$(".menu-item-has-children a:contains('Kho Mã Nguồn')").parent().addClass("current-menu-item"));